{
	"name": "@younho9/xo-configs",
	"version": "0.7.0",
	"description": "Personal XO configs",
	"keywords": [
		"xo",
		"xo-config",
		"eslint",
		"eslint-config"
	],
	"bugs": {
		"url": "https://github.com/younho9/xo-configs/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/younho9/xo-configs.git"
	},
	"license": "MIT",
	"author": {
		"name": "Younho Choo",
		"email": "yo+github@younho9.com",
		"url": "https://younho9.dev"
	},
	"sideEffects": false,
	"exports": {
		".": "./src/index.cjs",
		"./react": "./src/react.cjs",
		"./next": "./src/next.cjs"
	},
	"files": [
		"src"
	],
	"scripts": {
