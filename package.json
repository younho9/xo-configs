		"eslint-plugin-react-hooks": "4.3.0"
	},
	"devDependencies": {
		"@commitlint/cli": "15.0.0",
		"@commitlint/config-conventional": "15.0.0",
		"eslint-plugin-cypress": "^2.12.1",
		"husky": "7.0.4",
		"xo": "0.48.0"
	},
	"peerDependencies": {
		"xo": "0.48.0"
	},
	"engines": {
		"node": ">=12.20"
	},
